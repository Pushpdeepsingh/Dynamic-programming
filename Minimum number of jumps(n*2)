    // Write C++ code here
int n=10;
int arr[10]={2,1,3,2,3,4,5,1,2,8};
int mini[10];
for(int i=0;i<10;i++){
    min[i]=10000000;
}
mini[0]=0;
for(int i=0;i<10;i++){
    cout<<min[i]<<endl;
}
for (int i=1;i<n;i++){
    for(int j=0;j<n;j++){
        if (i<=j+arr[j]){
            mini[i]=min(mini[i],mini[j]+1)
        }
    }
}
return mini[n-1];
